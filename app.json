{
  "expo": {
    "name": "BankStatementGen",
    "slug": "bankstatement-gen",
    "version": "0.1.0",
    "scheme": "bankstatementgen",
    "orientation": "portrait",
    "userInterfaceStyle": "automatic",
    "assetBundlePatterns": [
      "**/*"
    ],
    "ios": {
      "supportsTablet": true
    },
    "android": {
      "package": "com.bankstatementgen.app"
    },
    "web": {
      "bundler": "metro",
      "favicon": "./assets/favicon.png"
    },
    "plugins": [
      "expo-router",
      "react-native-vision-camera"
    ],
    "experiments": {
      "typedRoutes": true
    }
  }
}
